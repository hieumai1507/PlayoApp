{
  "expo": {
    "name": "playoApp",
    "slug": "playoApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://playo-website.gumlet.io/playo-website-v2/logos-icons/new-logo-playo.png?q=50",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://playo-website.gumlet.io/playo-website-v2/logos-icons/new-logo-playo.png?q=50",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://playo-website.gumlet.io/playo-website-v2/logos-icons/new-logo-playo.png?q=50"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://playo-website.gumlet.io/playo-website-v2/logos-icons/new-logo-playo.png?q=50",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
